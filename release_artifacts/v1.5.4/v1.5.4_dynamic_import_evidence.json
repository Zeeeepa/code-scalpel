{
  "version": "1.5.4",
  "release_name": "DynamicImports",
  "date": "2025-12-14",
  "detection": {
    "importlib.import_module": "supported",
    "__import__": "supported",
    "variable_args": "resolved when simple string vars; otherwise marked LAZY",
    "lazy_marker": "?",
    "frameworks": ["django_INSTALLED_APPS", "flask_blueprints"]
  },
  "patterns_covered": [
    "importlib.import_module(\"math\")",
    "__import__(\"json\")",
    "mod = 'datetime'; importlib.import_module(mod)",
    "bp = Blueprint('bp', __name__); app.register_blueprint(bp)",
    "INSTALLED_APPS = ['django.contrib.admin', 'django.contrib.auth', 'myapp']"
  ],
  "limitations": [
    "Non-literal call arguments beyond simple name->string assignment are marked LAZY",
    "No resolution of computed strings or f-strings"
  ]
}
